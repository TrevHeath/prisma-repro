datasource db {
  provider = "postgresql"
  url      = "postgresql://postgres:postgres@localhost:5432/prisma-debug"
}

generator prisma_client {
  provider = "prisma-client-js"
}

model User {
  id        String   @default(cuid()) @id
  email     String   @unique
  birthDate DateTime
  metadata  Json
  posts     Post[]
}

model Post {
  id      String  @default(cuid()) @id
  authors User[]  @relation(references: [id])
  message String?
}
